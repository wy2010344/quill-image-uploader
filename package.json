{
    "name": "@wangyang2010344/quill-image-uploader",
    "version": "1.2.3",
    "repository": "https://github.com/noeloconnell/quill-image-uploader.git",
    "description": "A module for Quill rich text editor to allow images to be uploaded to a server instead of being base64 encoded",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "keywords": [
        "quill",
        "quilljs",
        "image",
        "upload"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "bugs": {
        "url": "https://github.com/wy2010344//quill-image-uploader/issues"
    },
    "homepage": "https://github.com/wy2010344//quill-image-uploader#readme",
    "module": "dist//quill-image-uploader.esm.js",
    "size-limit": [
        {
            "path": "dist//quill-image-uploader.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist//quill-image-uploader.esm.js",
            "limit": "10 KB"
        }
    ],
    "author": "wy2010344",
    "license": "MIT",
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@types/quill": "^2.0.9",
        "husky": "^7.0.4",
        "quill": "*",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.6.2"
    },
    "peerDependencies": {
        "quill": "*"
    }
}